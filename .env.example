# Database Configuration
ConnectionStrings__DefaultConnection=Host=localhost;Database=UserAuthDB;Username=postgres;Password=your_password_here

# JWT Configuration
Jwt__SecretKey=your-super-secret-jwt-key-that-is-at-least-32-characters-long
Jwt__Issuer=UserAuthAPI
Jwt__Audience=UserAuthAPI-Client
Jwt__ExpirationMinutes=60

# Google OAuth Configuration
Google__ClientId=your-google-client-id.googleusercontent.com

# Application Settings
ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=https://localhost:7001;http://localhost:5001

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft.AspNetCore=Warning